score = input("Enter Score: ")
try :
    grade=float(score)
    if 1.0>grade>=0.90:
        print('A')
        
    elif 0.9>grade>=0.8:
       		 print('B')
            
    elif 0.8>grade>=0.7:
             print('C')
            
    elif 0.7>grade>=0.6:
            print("D")
            
    elif grade<=0.6:
                print("F")
                
    else :
            exit()
except:
    print("enter valid number")
     
       
